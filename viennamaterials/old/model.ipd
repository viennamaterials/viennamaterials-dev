  Conductor
  {
     // The base section is virtual, i.e. there is no default conductivity
     class     = "Conductor";
     materials = "";

     conductivity = "Const";
     Conductivity { Const { s = 0.0 "S/m"; } }
  }

  Silicide : Conductor;

