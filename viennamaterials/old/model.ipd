  <Insulator> : BaseMaterial
  {
     class      = "Insulator";
     materials  = class;

     permittivity = "Pure"; 
     Permittivity
     {
	Pure
	{
	   epsr = 1.0;
	}
     }

     oxideTrap = "None";
     OxideTrap
     {
        Pure
        {
           Nt        = 2e15 "cm^-3";  // trap concentration
           type      = "neutral";     // charge state
           occupancy = 0.1;           // trap occupancy
           energy    = 2 "eV";        // trap energy level below conduction band
        }
        Charge : Pure
        {
           +C     = 0.0;   // Conductance after breakdown
           +alpha = 0.0;   // Voltage exponent after breakdown
        }
     }

     interfaceCharge = "None";
     InterfaceCharge
     {
        Pure
        {
           Di = 1e12 "cm^-2";   // interface charge density
        }
     }

     thermalHeatFlux = "Ltm_Pure";
     ThermalHeatFlux 
     {
	Ltm_Pure
	{
	   kappa = 1.38 "W/K m";  // SiO2 value
	   alpha = 0.33;          // SiO2 value
	}
     }

     latticeHeatCapacity = "Ltm_Pure";
     LatticeHeatCapacity
     {
	Ltm_Pure
	{
	   heat  = 709 "J/K kg";   // SiO2 value
	   dheat = 696 "J/K kg";   // SiO2 value
	   alpha = 1.5;            // SiO2 value
	}
     }

     massDensity = "Pure";
     MassDensity
     {
	Pure
	{
	   rho = 2.203 "g/cm^3";  // SiO2 value
	}
     }

     contact = "None";           // Use "Ohmic" for thermal contacts
     Contact
     {
	Ohmic : ^^IdealConductor.Contact.Ohmic
        {
	  type = "Thermal";
        }
     }
     Electron
     {
        tunnel = "None";
        Tunnel
        {
           _Common_
           {
              consistent = yes;
           }
           FNPure : _Common_
           { 
              +a          = 994.6316e-9 "A/V^2";  // value for m=0.5m_ox, Phi_b = 3.1eV
              +b          = 2.635706e10 "V/m";    // value for m=0.5m_ox, Phi_b = 3.1eV
           }
           FrenkelPoole : _Common_
           {  
              +a          = 994.6316;    // value for m=0.5m_ox, Phi_b = 3.1eV
              +b          = 2e-24;       // value for m=0.5m_ox, Phi_b = 3.1eV
              +trapNrg    = 1.5 "eV";
           }
           FNLenzlingerSnow : _Common_
           {
              +mOx        = 0.5;       
              +phiB       = 0 "eV";
           }
           DTSchuegraf : _Common_
           {
              +mOx        = 0.5;       
              +phiB       = 0 "eV";
           }
           TsuEsaki : _Common_
           {
              +ccb        = yes;             // consider conduction band tunneling                      
              +cvb        = no;              // consider valence band tunneling                      
              +tat        = no;              // consider trap-assisted tunneling
	      +mtat       = no;              // consider multi-trap-assisted tunneling
              +mOx        = 0.5;             // carrier mass in the dielectric
              +tcType     = "numericalWKB";  // "analyticalWKB,qtbm"
              +dfType     = "fermi";         // "heated"
              +trapRadius = 0.25 nm;         // trap radius
              +dNrg       = 10 "meV";        // energy step width for numerical integration
              +huangRhys  = 65;              // Huang-Rhys factor for trap-assisted tunneling
              +phononNrg  = 0.030 eV;        // Phonon energy for trap-assisted tunneling 
              +imageForce = no;              // image force correction
              +conc       = no;              // calculate concentration
              +deltaX     = 0.1 "nm";       // spacing for qtbm method
              +checkBd    = no;              // check for breakdown
              +J0         = 1 "A*cm^-2";     // breakdown current prefactor
              +beta       = 2.0;             // breakdown current exponent                     
              +connL      = 5 "nm";          // connection length for qtb method
              +singlePoint = no;
           }
        }
     }
     Hole
     {
        tunnel = "None";
        Tunnel   
        {
           _Common_
           {
              consistent = yes;
           }
           FNPure : _Common_
           {
              +a          = 4.013e-7  "A/V^2";   // value for m=0.8m_ox, Phi_b = 4.8eV
              +b          = 6.4216e10 "V/m";     // value for m=0.8m_ox, Phi_b = 4.8eV
           }
           FrenkelPoole : _Common_
           {  
              +a          = 0.0;     // FrenkelPoole model used for electrons only
              +b          = 0.0;
              +trapNrg    = 0.0 "eV";
           }
           FNLenzlingerSnow : _Common_
           {
              +mOx        = 0.8;       
              +phiB       = 0 "eV";
           }
           DTSchuegraf : _Common_
           {
              +mOx        = 0.8;       
              +phiB       = 0 "eV";
           }
           TsuEsaki : _Common_
           {
              +ccb        = yes;             // consider conduction band tunneling                      
              +cvb        = no;              // consider valence band tunneling                      
              +tat        = no;              // consider trap-assisted tunneling              
	      +mtat       = no;              // consider multi-trap-assisted tunneling
              +mOx        = 0.8;             // carrier mass in the dielectric                
              +tcType     = "numericalWKB";  // "analyticalWKB,qtbm"                          
              +dfType     = "fermi";         // "heated"                                      
              +trapRadius = 0.25 nm;         // trap radius                                   
              +dNrg       = 20 "meV";        // energy step width for numerical integration   
              +huangRhys  = 65;              // Huang-Rhys factor for trap-assisted tunneling 
              +phononNrg  = 0.030 eV;        // Phonon energy for trap-assisted tunneling     
              +imageForce = no;              // image force correction                        
              +conc       = no;              // calculate concentration                       
              +deltaX     = 0.1 "nm";       // spacing for qtbm method                       
              +checkBd    = no;              // check for breakdown                           
              +J0         = 1 "A*cm^-2";     // breakdown current prefactor                   
              +beta       = 2.0;             // breakdown current exponent                    
              +connL      = 5 "nm";          // connection length for qtb method
              +singlePoint = no;
           }
        }
     }


     bandGapEnergy = "Ltm_Pure";
     BandGapEnergy
     {
	Ltm_Pure
	{
	   Eg0   =  9.0 eV;
	   Eoffs = -5.21 eV;  // offset energy    
	   alpha =  4.73e-4 "eV/K"; 
	   beta  =  636 K;          
	}
     }


  }

