  }

  // ==========================================================================
  // ==========================================================================
  // ==========================================================================
  // 
  //             Semiconductor
  //
  // ==========================================================================
  // ==========================================================================
  // ==========================================================================

  <Semiconductor> : BaseMaterial
  {
     class      = "Semiconductor";
     materials  = class;

     fermiIntegralP12 = "MM6";   // PRIVATE
     fermiIntegralM12 = "MM6";   // PRIVATE
     fermiIntegralI12 = "MM6";   // PRIVATE
     fermiRatioP12M12 = "MM6";   // PRIVATE
     fermiRatioP32P12 = "MM6";   // PRIVATE
     test             = "Fermi"; // PRIVATE
     quantumCorrection= "None";

     QuantumCorrection
     {
	Haensch
	{
	   z0     = 1.0 "nm";
	   hLimit = 1.0e-5;
	   lFit   = 1.0;
	}
	VanDort
	{
	   beta = 4.1e-8 "eV*cm";
	   zref = 1.0 "nm";
	}
	IMLDA
	{
	   p1       = 4.026e-2;
	   p2       = 71.442;
	   p3       = 1.353;
           mt       = 0.19;
	   ml       = 0.92;
        }
     }
     Electron
     {
        interfaceDD = "CQFL";
        interfaceHD = "CQFL";

        InterfaceDD
        {
	   TFE
	   {
	      d = 7e-9 m;
	      scaleFactor = 1.0;
	   }
        }
        InterfaceHD : InterfaceDD;

        distribution = "ColdMaxwellian";
        Distribution
        {
           ColdMaxwellian
           {
              T   = 300 K;
              exp = 1.0;
           }
           HeatedMaxwellian  // T from carrier temperature
           {
              exp = 1.0;
           }
           NonMaxwellian  // no parameters needed
           {
           }
        }
        tunnel = "None";
        Tunnel
        {
           _Common_
           {
              consistent = yes;
           }
           FNPure : _Common_
           {  // values for m=0.5m_ox, Phi_b = 3.1eV
              +a          = 994.6316e-9 "A/V^2"; 
              +b          = 2.635706e10 "V/m";
           }
           FrenkelPoole : _Common_
           {  
              +a          = 994.6316; 
              +b          = 2e-24;
              +trapNrg    = 1.5 "eV";
           }
           FNLenzlingerSnow : _Common_
           {
              +mOx        = 0.5;       
              +phiB       = 0 "eV";
           }
           DTSchuegraf : _Common_
           {
              +mOx        = 0.5;       
              +phiB       = 0 "eV";
           }
           TsuEsaki : _Common_
           {
              +ccb        = yes;             // consider conduction band tunneling                      
              +cvb        = no;              // consider valence band tunneling                      
              +tat        = no;              // consider trap-assisted tunneling              
	      +mtat       = no;              // consider multi-trap-assisted tunneling
              +mOx        = 0.5;             // carrier mass in the dielectric                
              +tcType     = "numericalWKB";  // "analyticalWKB,qtbm"                          
              +dfType     = "fermi";         // "heated"                                      
              +trapRadius = 0.25 nm;         // trap radius                                   
              +dNrg       = 10 "meV";        // energy step width for numerical integration   
              +huangRhys  = 65;              // Huang-Rhys factor for trap-assisted tunneling 
              +phononNrg  = 0.030 eV;        // Phonon energy for trap-assisted tunneling     
              +imageForce = no;              // image force correction                        
              +conc       = no;              // calculate concentration                       
              +deltaX     = 0.1 "nm";        // spacing for qtbm method                       
              +checkBd    = no;              // check for breakdown                           
              +J0         = 1 "A*cm^-2";     // breakdown current prefactor                   
              +beta       = 2.0;             // breakdown current exponent                    
              +connL      = 5 "nm";          // connection length for qtb method
              +singlePoint = no;
           }
        }

     }
     Hole : Electron
     {
        tunnel = "None";
        Tunnel
        {
           _Common_
           {
              consistent = yes;
           }
           FNPure : _Common_
           {  // values for m=0.8m_ox, Phi_b = 4.8eV
              +a          = 4.013e-7  "A/V^2"; 
              +b          = 6.4216e10 "V/m";
           }
           FrenkelPoole : _Common_
           {  
              +a          = 0.0;   // FrenkelPoole model used for electrons only
              +b          = 0.0;
              +trapNrg    = 0.0 "eV";
           }
           FNLenzlingerSnow : _Common_
           {
              +mOx        = 0.8;       
              +phiB       = 0 "eV";
           }
           DTSchuegraf : _Common_
           {
              +mOx        = 0.8;       
              +phiB       = 0 "eV";
           }
           TsuEsaki : _Common_
           {
              +ccb        = yes;             // consider conduction band tunneling                      
              +cvb        = no;              // consider valence band tunneling                      
              +tat        = no;              // consider trap-assisted tunneling               
	      +mtat       = no;              // consider multi-trap-assisted tunneling
              +mOx        = 0.8;             // carrier mass in the dielectric                 
              +tcType     = "numericalWKB";  // "analyticalWKB,qtbm"                           
              +dfType     = "fermi";         // "heated"                                       
              +trapRadius = 0.25 nm;         // trap radius                                    
              +dNrg       = 10 "meV";        // energy step width for numerical integration    
              +huangRhys  = 65;              // Huang-Rhys factor for trap-assisted tunneling  
              +phononNrg  = 0.030 eV;        // Phonon energy for trap-assisted tunneling      
              +imageForce = no;              // image force correction                         
              +conc       = no;              // calculate concentration                        
              +deltaX     = 0.1 "nm";       // spacing for qtbm method                        
