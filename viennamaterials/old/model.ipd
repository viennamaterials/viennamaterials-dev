  Semiconductor_Insulator
  {
     class      = "Semiconductor_Insulator";
     materials  = "Semiconductor";
     materials2 = "Insulator";

     interfaceTrapDOS = "Box";
     InterfaceTrapDOS
     {
        _Common_
        {
           Emax  =  0 eV;  // the mobility edge
           Emin  = -1 eV;
           Nt    =  1e10 "cm^-2";  // srh active contribution
           Nf    =  0 "cm^-2";     // fixed charge
           //Mt    = ~Num.EnergyDiscretization.intervals;
           useEc = no;
           useEv = no;
        }
        <Single> : _Common_
        {
           Et  = - 0.3 eV;
           tch = + 1;
           Mt  =   1;
        }
        <Box> : _Common_
        {
           useEc = yes;
           useEv = yes;
           tch   = + 1; // + 1 gives donor-like traps, while -1 results in acceptor-like traps
        }
        <Exponential> : _Common_
        {
           E0  =   0.3 eV;  // reference energy
           tch = + 1;
        }
        <Gaussian> : _Common_
        {
           useEc  = yes;
           useEv  = yes;
           E0    =   0.3 eV;      // reference energy for exponential tail states
           Em    = - 0.3 eV;      // average energy
           sigma =   0.3 "eV^2";  // variance
           r     =   1;           // ratio between Gaussian and exponential distribution
           tch   = + 1;
        }
        <DoubleFermiDer> : _Common_
        {
           useEc  = yes;
           useEv  = yes;
           Em1    = -0.235 eV;    // average energy
           Em2    = -0.85  eV;    // average energy
           sigma1 = 0.08 "eV^2";  // variance
           sigma2 = 0.08 "eV^2";  // variance
           tch    = + 1;
        }
     }

     interfaceTrap = "None";
     InterfaceTrap
     {
	Simple
	{
           onlyCharge     = yes;  // just add to Poisson
           dynamic        = no;   // if no, use equilibrium dynamics
           // fixedNitCharge = no;   // add a charge corresponding to nit/2

           ste = 1.0e-19 "m^2";   // actually only needed for SRH
           sth = 1.0e-19 "m^2";
           //Mt  = ~Num.EnergyDiscretization.intervals; // remove that
	}
	<SRH> : Simple
	{
	}
     }

     interfaceTrapGeneration = "None";
     InterfaceTrapGeneration
     {
	_Common_
	{
	   freeze = no;             // if set to yes generation of traps is stopped
	   Nit0   = 0.0 "cm^-2";    // initial number of broken bonds
	}
	<General> : _Common_
	{
	   N0    = 1.24e14 "cm^-2";     // passivated interface bonds

	   kF11  = 0 "s^-1";            // forward reaction rate (degradation)
	   EaF11 = 0 "eV";              // Arrhenius activation energy
	   kR11  = 0 "cm^3 s^-1";       // reverse reaction rate (annealing)
	   EaR11 = 0 "eV";              // Arrhenius activation energy
	   kF12  = 0 "s^-1";            // forward reaction rate (degradation)
	   EaF12 = 0 "eV";              // Arrhenius activation energy
	   kR12  = 0 "cm^3 s^-1";       // reverse reaction rate (annealing)
	   EaR12 = 0 "eV";              // Arrhenius activation energy

	   kF21  = 0 "s^-1";            // forward reaction rate (degradation)
	   EaF21 = 0 "eV";              // Arrhenius activation energy
	   kR21  = 0 "cm^3 s^-1";       // reverse reaction rate (annealing)
	   EaR21 = 0 "eV";              // Arrhenius activation energy
	   kF22  = 0 "s^-1";            // forward reaction rate (degradation)
	   EaF22 = 0 "eV";              // Arrhenius activation energy
	   kR22  = 0 "cm^3 s^-1";       // reverse reaction rate (annealing)
	   EaR22 = 0 "eV";              // Arrhenius activation energy

	   kF31  = 0 "s^-1";            // forward reaction rate (degradation)
	   EaF31 = 0 "eV";              // Arrhenius activation energy
	   kR31  = 0 "cm^3 s^-1";       // reverse reaction rate (annealing)
	   EaR31 = 0 "eV";              // Arrhenius activation energy
	   kF32  = 0 "s^-1";            // forward reaction rate (degradation)
	   EaF32 = 0 "eV";              // Arrhenius activation energy
	   kR32  = 0 "cm^3 s^-1";       // reverse reaction rate (annealing)
	   EaR32 = 0 "eV";              // Arrhenius activation energy

	   kF512  = 0 "s^-1";           // forward reaction rate (degradation)
	   EaF512 = 0 "eV";             // Arrhenius activation energy
	   kR512  = 0 "cm^3 s^-1";      // reverse reaction rate (annealing)
	   EaR512 = 0 "eV";             // Arrhenius activation energy

	   kF521  = 0 "s^-1";           // forward reaction rate (degradation)
	   EaF521 = 0 "eV";             // Arrhenius activation energy
	   kR521  = 0 "cm^3 s^-1";      // reverse reaction rate (annealing)
	   EaR521 = 0 "eV";             // Arrhenius activation energy

	   E0    = 3 "MV/cm";           // reference electric field for generation
	   p0    = 1e20  "cm^-3";       // reference hole concentration for generation

           rf    = 1;                   // ratio at which fixed charges are generated
                                        // Nf = rf * Nit

           biasdep11 = no;
           biasdep12 = no;
           biasdep21 = no;
           biasdep22 = no;
           biasdep31 = no;
           biasdep32 = no;

           I0 = 1 A; // very clever guess, sorry
           an = 0;
           ap = 0;
           bn = 1;
           bp = 1;
	}
     }

     hydrogen0Interface = "None";
     Hydrogen0Interface
     {
	Const
	{
           Hc1 = 1e-9 "cm^-3";   // constant hydrogen concentration in semiconductor, don't set to zero
           Hc2 = 1e-9 "cm^-3";   // constant hydrogen concentration in insulator, don't set to zero
	}
	Couple {} // no parameters, couples two segments
	Flux
        {
           s1 = 0 "cm/s";  // surface recombination velocity semiconductor
           s2 = 0 "cm/s";  // surface recombination velocity insulator
        }
     }

     hydrogen2Interface = "None";
     Hydrogen2Interface : Hydrogen0Interface;

     hydrogenPInterface = "None";
     HydrogenPInterface : Hydrogen0Interface;
  }

