  // ==========================================================================
  // ==========================================================================
  // ==========================================================================
  // ==========================================================================
  // ==========================================================================
  // ==========================================================================
  //
  //   Every section has a materials keyword, which determines the materials
  //   these defaults belong to. Can be a comma-separated wildcarded list, e.g.
  //
  //   materials = "Si,SiGe";  //   only Si and SiGe
  //   materials = "Ga*";      //   all kind of GaAs, GaInAs, ...
  //
  //   This section or an equivalent user-defined section has to be used in the
  //   Device.Phys.SegmentDefaults section, which is the actually read section.
  //   The first subsection in the Device.Phys.SegmentDefaults section matching
  //   the material of the segment will be used, so use the materials = "*" in 
  //   the last subsection.
  //
  //   Note that the name of the section is arbitrary, only the materials
  //   keyword and the order matter.
  //
  // ==========================================================================
  // ==========================================================================


MaterialDefaults
{
  // ==========================================================================
  // ==========================================================================
  // ==========================================================================
  // 
  //             Base Material
  //
  //             Contains models which might be applicable to all materials
  //
  // ==========================================================================
  // ==========================================================================
  // ==========================================================================

  BaseMaterial
  {
     class     = "BaseMaterial";
     materials = ""; //"Semiconductor,Insulator";

     hydrogen0Transport = "Diffusion";
     Hydrogen0Transport
     {
	Diffusion
	{
	   D0         = 5.64e-4 "cm^2 s^-1"; // Arrhenius prefactor
	   Ea         = 0.45 "eV";           // Arrhenius activation energy
           oneD       = no;
	   freeze     = no; // if set yes the transport is turned off
           consistent = no; // add charges of mobile carriers to Poisson's equation

           initVal    = 1e-9 "cm^-3";

           Z = 0.0;  // charge state of mobile species
	}
     }

     hydrogen2Transport = "Diffusion";
     Hydrogen2Transport
     {
	Diffusion
	{
	   D0         = 5.64e-4 "cm^2 s^-1"; // Arrhenius prefactor
	   Ea         = 0.45 "eV";           // Arrhenius activation energy
           oneD       = no;
	   freeze     = no; // if set yes the transport is turned off
           consistent = no; // add charges of mobile carriers to Poisson's equation

           initVal    = 1e-9 "cm^-3";

           Z          = 0.0;  // charge state of mobile species
	}
     }

     hydrogenPTransport = "DriftDiffusion";
     HydrogenPTransport : Hydrogen2Transport
     {
	+DriftDiffusion : Diffusion
	{
	   D0 = 5.64e-4 "cm^2 s^-1"; // Arrhenius prefactor
	   Ea = 0.45 "eV";           // Arrhenius activation energy
	   Z  = +1.0;
	}
     }

     hydrogenTraps = "None";
     HydrogenTraps
     {
        _Common_
        {
           occ  = yes; // consider occupancy of trap levels
           stat = no;  // use equilibrium assumption for trap dynamics

           // remove those
	   //chargedTrapped = yes;
           //Z = +1;
        }
        <General> : _Common_
        {
           Nc0 = 1e16 "cm^-3";
           nu0 = 300 Hz;
           Nc2 = 1e16 "cm^-3";
           nu2 = 300 Hz;
           NcP = 1e16 "cm^-3";
           nuP = 300 Hz;
        }
     }

     hydrogenTrapDOS = "Exponential";
     HydrogenTrapDOS
     {
        _Common_
        {
           Emax  =  0 eV;  // the mobility edge
           Emin  = -1 eV;
           Nt    =  1e10 "cm^-3";
           Nf    =  0 "cm^-3"; // fixed contribution, currently not implemented
           //Mt    = ~Num.EnergyDiscretization.intervals;
           useEc = no;
           useEv = no;
        }
        <Single> : _Common_
        {
           Et  = - 0.3 eV;
           tch = + 1;
           Mt  =   1;
        }
        <Box> : _Common_
        {
           tch = + 1;
        }
        <Exponential> : _Common_
        {
           E0  =   0.3 eV;  // reference energy
           tch = + 1;
        }
        <Gaussian> : _Common_
        {
           E0    =   0.3 eV;      // reference energy for exponential tail states
           Em    = - 0.3 eV;      // average energy
           sigma =   0.3 "eV^2";  // variance
           r     =   1;           // ratio between Gaussian and exponential distribution
           tch   = + 1;
        }
     }

     oxideChargeCollector = "None";
     OxideChargeCollector
     {
        Default
        {
           tox = -1;
        }
     }

  }

  // ==========================================================================
  // ==========================================================================
  // ==========================================================================
  // 
  //             IdealConductors 
  //
  // ==========================================================================
  // ==========================================================================
  // ==========================================================================

  IdealConductor
  {
     // PRIVC we use "Conductor" here instead of the Conductor section
     // as we use infinity as default conductivity.
     class      = "IdealConductor";
     materials  = "IdealConductor,Conductor";

     contact = "Ohmic";          // Use "None" for non-contact segments
     Contact
     {
	_Common_
        {
	  // This section contains the keywords which are 
	  // supported by the contact model base class and
	  // are therefore available for all contact models.

	  type = "Voltage";      // Determines the basic contact type. 
                         	 // Might be "Voltage", "Current",
		 		 // or "Charge" controlled. For thermal
				 // contacts use "Thermal", for a combination
				 // of both separate with a comma.
          R    = 0.0 Ohm;        // Ohmic contact resistance
          Rth  = 0.0 "K cm^2/W"; // Thermal contact resistivity
          Rg   = 0.0 "K/W";      // Global thermal contact resistance
          Rth3d  = 0.0 "K cm^2/W"; // Thermal width contact resistivity 
	  only3d = no;             // Don't work as 2d contact, just
				   // pick up heat from the 3rd dimension
	  Ew   = 0.0 eV;         // Work function difference
	  qfl  = "";             // Segments where contact controls qfl
  				 // ENDC 
                                 // NOTE: Use only when the potential on
  				 // this contact is significantly different
  				 // from zero since it deteriorates the
  				 // condition of the linear system.
  				 // However, for high bias on this contact
  				 // it is absolutely necessary.

	  Z0     = 50.0 Ohm;       // characteristic impedance
	  parR   =  0.0 Ohm;       // parasitic resistance
          parL   =  0.0 pH;        // parasitic inductance
	  parC   =  0.0 fF;        // parasitic capacitance
        }

	Ohmic : _Common_;

        Schottky : _Common_
        {
          +rve  = 0.0 "cm/s";    // Electron recombination velocity
          +rvh  = 0.0 "cm/s";    // Hole recombination velocity
          +me   = 0.0;           // electron mass / free electron mass
          +mh   = 0.0;           // hole mass     / free electron mass
        }
        PolySilicon : _Common_
        {
	  +delta = 5e-10 m;      // Oxide thickness
	  +rv    = 62e3 "cm/s";  // Minority carrier recombination velocity	
	  +sigma = 2.8 "S/um^2"; // Majority carrier conductivity	  
	  +epsr  = 3.9;	         // Relative permittivity of oxide
        }
     }

     // In metals, there is no carrier heating 
     Electron  
     {  // PRIVATE
        distribution = "ColdMaxwellian";
        Distribution
        {
           ColdMaxwellian
           {
              T   = 300 K;
              exp = 1.0;
           }
        }
     }
     Hole  
     {  // PRIVATE
        distribution = "ColdMaxwellian";
        Distribution
        {
           ColdMaxwellian
           {
              T   = 300 K;
              exp = 1.0;
           }
        }
     }
  }

  IdealConductors : IdealConductor
  {
     materials = "n-Poly,Poly,Al,Aluminum,Metal,Cu,Ti";
  }

